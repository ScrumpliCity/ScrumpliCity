{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "migrateDB": "cd ../backend && php artisan migrate:fresh",
    "startAll": "concurrently \"cd ../backend && php artisan queue:work\" \"cd ../backend && php artisan reverb:start\" \"cd ../backend && php artisan serve\" \"npm run dev\"",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "format": "prettier . --write",
    "check": "prettier . --check",
    "prepare": "cd .. && husky frontend/.husky"
  },
  "dependencies": {
    "@nuxt/ui": "^2.21.0",
    "@nuxtjs/i18n": "^9.2.0",
    "@nuxtjs/seo": "^2.1.1",
    "@pinia/nuxt": "^0.9.0",
    "concurrently": "^9.1.2",
    "nuxt": "^3.15.4",
    "nuxt-auth-sanctum": "^0.5.4",
    "nuxt-laravel-echo": "^0.2.1",
    "nuxt-svgo": "^4.0.14",
    "pinia": "^2.3.1",
    "vue": "latest",
    "vue-router": "latest"
  },
  "devDependencies": {
    "@iconify-json/akar-icons": "^1.2.2",
    "@iconify-json/ic": "^1.2.2",
    "@iconify-json/mdi": "^1.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "prettier": "3.5.0",
    "prettier-plugin-tailwindcss": "^0.6.11"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
